{
  "name": "mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:dev": "react-native run-android",
    "android:prod": "react-native run-android --variant=release",
    "ios:dev": "react-native run-ios",
    "ios:prod": "react-native run-ios --configuration Release",
    "ios:dev:device": "react-native run-ios --device \"Palash's iPhone\"",
    "ios:prod:device": "react-native run-ios --device \"Palash's iPhone\" --configuration Release",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "pods": "cd ios && pod install && cd ..",
    "xcode": "open ios/HelloWorld.xcworkspace"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.7.1",
    "@react-native-community/masked-view": "^0.1.6",
    "@react-native-community/netinfo": "^5.3.3",
    "@react-navigation/native": "^5.0.0",
    "@react-navigation/native-stack": "^5.0.0",
    "@react-navigation/stack": "^5.0.0",
    "axios": "0.19.2",
    "react": "16.12.0",
    "react-native": "^0.61.5",
    "react-native-device-info": "^5.4.4",
    "react-native-gesture-handler": "^1.5.3",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-picker-select": "^6.3.4",
    "react-native-reanimated": "^1.7.0",
    "react-native-safe-area-context": "^0.6.2",
    "react-native-screens": "^2.0.0-beta.2",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^9.13.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^0.0.7",
    "@svgr/babel-plugin-transform-react-native-svg": "^5.0.1",
    "babel-jest": "^25.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^6.5.1",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-test-renderer": "16.12.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
